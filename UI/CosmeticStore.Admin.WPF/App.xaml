             xmlns:local="clr-namespace:CosmeticStore.Admin.WPF"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/DefaultStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
